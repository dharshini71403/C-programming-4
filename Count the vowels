#include <stdio.h>
#include <string.h>

int main() {
    char str[1000];
    int count = 0;
    scanf("%s", str);
    for (int i = 0; str[i] != '\0'; i++) {
        char ch = str[i];
        if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u') {
            count++;
        }
    }
    printf("%d\n", count);

    return 0;
}
